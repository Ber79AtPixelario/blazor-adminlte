@namespace Blazor.AdminLte
<div class="card">
    <div class="card-body">
        <h5 class="card-title">@Title</h5>
        <p class="card-text">
            @Contents
        </p>
    </div>
</div>
@code {
    [Parameter]
    public RenderFragment Title { get; set; }
    [Parameter]
    public RenderFragment Contents { get; set; }
}
